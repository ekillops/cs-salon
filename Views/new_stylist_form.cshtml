@{
  Layout = "Shared/_Layout.cshtml";
}
<br>
<h3>NEW STYLIST</h3>
<br>
<form action="/stylists" method="post">
  <label>Stylist Details</label>
  <input class="form-control" type="text" name="stylist-name" placeholder="Name" required>
  <input class="form-control" type="text" name="stylist-phone-number" placeholder="Phone Number" required>
  <br>
  <button type="submit" class="btn btn-default btn-md">Submit</button>
</form>
