@{
  Layout = "Shared/_Layout.cshtml";
}
<br>
<h3>SEARCH RESULTS</h3>
<br>
<p>@Model.Count items match your search...</p>
<br>
@if (Model.Count != 0)
{
  @foreach (var result in Model)
  {
    <p>@result.GetName()</p>
    @if(result is Stylist)
    {
      <p><a href="/stylists/@result.GetId()"><small>View page</small></a></p>
    }
    else
    {
      <p><a href="/clients/@result.GetId()"><small>View page</small></a></p>
    }
    <br>
  }
}
